<h1>Listing users</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Ph no</th>
    <th>Skill1</th>
    <th>Skill2</th>
    <th>Skill3</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.name %></td>
    <td><%= user.ph_no %></td>
    <td><%= user.skill1 %></td>
    <td><%= user.skill2 %></td>
    <td><%= user.skill3 %></td>
    <td><%= link_to 'Show', user %></td>
    <td><%= link_to 'Edit', edit_user_path(user) %></td>
    <td><%= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New User', new_user_path %>
